<?xml version="1.0" encoding="UTF-8"?>
<jbi:jbi version="1.0"
         xmlns:jbi="http://java.sun.com/xml/ns/jbi"
         xmlns:petalsCDK="http://petals.ow2.org/components/extensions/version-5"
         xmlns:soap="http://petals.ow2.org/components/soap/version-4"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:technique="http://ausy.fr/training/petals/bibliotheque/ajouter/technique/1.0">
    <jbi:services binding-component="true">
        <jbi:consumes interface-name="technique:AjouterLivreTechnique"
                      service-name="technique:AjouterlLivreService">
            <petalsCDK:timeout/>
            <petalsCDK:mep xsi:nil="true"/>
            <soap:service-name>AjouterlLivreServiceTechnique</soap:service-name>
            <soap:mode>SOAP</soap:mode>
            <soap:enable-http-transport>true</soap:enable-http-transport>
            <soap:enable-jms-transport>false</soap:enable-jms-transport>
        </jbi:consumes>
    </jbi:services>
</jbi:jbi>
